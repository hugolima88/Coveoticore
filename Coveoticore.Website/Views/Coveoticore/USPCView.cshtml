@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{
    if (Model != null)
    {
        <div>@Html.Sitecore().Field("SingleLineText")</div>
        <div>@Html.Sitecore().Field("MultiLineText")</div>
        <div>@Html.Sitecore().Field("DateTime")</div>
        <div>@Html.Sitecore().Field("Number")</div>
        <div>@Html.Sitecore().Field("Image")</div>
    }
}
